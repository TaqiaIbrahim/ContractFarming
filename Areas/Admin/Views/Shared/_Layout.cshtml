<script>
    $(document).ready(function () {
        $.getJSON("/Admin/Home/GetCountNotification", function (data) {
            console.log(data);
            $("#newNotificationNo").text(data);
        });



        $('#bull').on("click", function () {
            $.getJSON("/Admin/Home/GetUnReadNotificationUser", function (data) {
                let v = $("#newNotificationNo").text();
                var item = "";

                for (var i = 0; i < v; i++) {

                    item += "<li onmouseenter=/'this.style.color = 'EFF3F7'/' onmouseleave=/'this.style.color = '000'/' ><h3>" + data.name[i] + "</h3> <p>" + data.desc[i] + "</p> </li><hr>";

                }

                $("#NotificationList").html(item);
                $("#newNotificationNo").text(0);


            });
            $('#AllNotic').on("click", function () {
                $.getJSON("/Admin/Home/GetReadNotificationUser", function (data) {
                    console.log(data);
                    var item = "";

                    for (var i = 0; i < 12; i++) {

                        item += "<li onmouseenter=/'this.style.color = 'EFF3F7'/' onmouseleave=/'this.style.color = '000'/' ><h3>" + data.name[i] + "</h3> <p>" + data.desc[i] + "</p> </li><hr>";
                        console.log(data.name[i]);
                    }

                    $("#AllNotificationList").html(item);



                });
            });





        });

    });
</script>
