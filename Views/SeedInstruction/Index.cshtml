@model IEnumerable<ContractFarming.Models.SeedInstructions>
<!-- Spinner Start -->
<!-- Spinner End -->
<!-- Navbar Start -->
<!-- Navbar End -->
<!-- Video Modal Start -->


<div class="modal modal-video fade"
     id="videoModal"
     tabindex="-1"
     aria-labelledby="exampleModalLabel"
     aria-hidden="true">

</div>
<!-- Video Modal End -->
<div class="card">
    <div class="container py-5" data-wow-delay="0.1s" style="visibility: visible; animation-delay: 0.1s;">
        <div class="section-title text-center position-relative pb-3 mb-5 mx-auto" style="max-width: 600px;">
            <h5 class="fw-bold text-primary text-uppercase">ارشادات البذور</h5>

        </div>
    </div>
    <div class="container-xxl py-5">
        <div class="table-responsive text-nowrap">
            <table class="table table-bordered">
                <thead style="    background-color:#2EB872; color: aliceblue;">
                    <tr>
                        <th style="    text-align: -webkit-center;">ارشادات البذور</th>

                    </tr>
                    <tr>
                        <th>
                            <ul class="list-unstyled users-list avatar-group d-flex align-items-center m-0" style="    justify-content: space-between;">
                                <li>الاسم</li>
                                <li>النوع</li>
                                <li>التصنيف</li>
                                <li>مدى النقاوة</li>
                                <li>اماكن التوافر</li>

                            </ul>
                        </th>
                    </tr>

                        @foreach (var item in Model)
                        {
                       
                <tbody class="table-border-bottom-0">

                    <tr>
                      
                            <th>
                                <ul class="list-unstyled users-list avatar-group d-flex align-items-center m-0" style="    justify-content: space-between;">
                                    <li>@item.SeedName</li>
                                    <li>@item.Type</li>
                                    <li>@item.Classification</li>
                                    <li>@item.RangeOfPurity</li>
                                    <li>@item.AvailablePlace</li>

                                </ul>


                            </th>
                       
                    </tr>

                </tbody>
                        }
            </table>
        </div>
    </div>
</div>


